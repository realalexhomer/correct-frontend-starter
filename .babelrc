{
  "presets": [
    ["@babel/preset-env", {"modules": false}],
    "@babel/preset-react",
    "@babel/typescript"
  ],
  "plugins": [
    "react-hot-loader/babel",
    "@babel/plugin-proposal-numeric-separator",
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread"
  ],
  "env": {
    "production": {
      "presets": ["minify"]
    },
    "test": {
      "presets": [
        "@babel/preset-env",
        "@babel/preset-react",
        "@babel/typescript"
      ],
      "plugins": [
        "@babel/plugin-proposal-numeric-separator",
        "@babel/proposal-class-properties",
        "@babel/proposal-object-rest-spread"
      ]
    }
  }
}